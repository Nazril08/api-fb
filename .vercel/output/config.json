{
  "version": 3,
  "images": {
    "sizes": [640, 750, 828, 1080, 1200, 1920, 2048, 3840],
    "domains": [],
    "minimumCacheTTL": 60,
    "formats": ["image/webp", "image/avif"]
  },
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/facebook"
    }
  ],
  "framework": {
    "version": "1.0.0"
  },
  "serverlessFunctions": {
    "api/facebook.js": {
      "runtime": "nodejs20.x",
      "maxDuration": 30
    }
  }
} 